#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <time.h>
#define N 1000

int main()
{
    int n1, n2, i, j, k=0;
    int a[N], a1[N], b[N], b1[N], c[N], c1[N];
    scanf("%d", &n1);
    if(n1==0)
    {
        a[0]=0;
        a1[0]=0;
    }
    for(i=0;i<n1;i++)
    {
        scanf("%d", &a[i]);
        scanf("%d", &a1[i]);
    }
    scanf("%d", &n2);
    if(n2==0)
    {
        b[0]=0;
        b1[0]=0;
    }
    for(i=0;i<n2;i++)
    {
        scanf("%d", &b[i]);
        scanf("%d", &b1[i]);
    }
    for(i=0;i<n1;i++)
    {
        for(j=0;j<n2;j++)
        {
            c[k]=a[i]*b[j];
            c1[k]=a1[i]+b1[j];
            k++;
        }
    }
    for(i=0;i<k-1;i++)
    {
        for(j=i+1;j<k;j++)
        {
            if(c1[i]==c1[j])
            {
                c[i]+=c[j];
                c[j]=0;
                c1[j]=0;
            }
        }
    }
    int max=c1[0], tem, tem1;
    for(i=0;i<k-1;i++)
    {
        max=c1[i];
        tem=i;
        for(j=i+1;j<k;j++)
        {
            if(c1[j]>max)
            {
                max=c1[j];
                tem=j;
            }
        }
        if(i!=j)
        {
            tem1=c1[i];
            c1[i]=c1[tem];
            c1[tem]=tem1;
            tem1=c[i];
            c[i]=c[tem];
            c[tem]=tem1;
        }
    }
    for(i=0;i<k;i++)
    {
        if(c[i]!=0)
        {
            printf("%d %d",c[i],c1[i]);
            i++;
            break;
        }
    }
    for(;i<k;i++)
    {
        if(c[i]!=0)
        {
            printf(" %d %d",c[i],c1[i]);
        }
    }   
    i=0;
    j=0;
    int flag = -1;
    if(a[0]==0||b[0]==0)
    {
        printf("0 0");
    }
        printf("\n");
    if(a[0]==0&&b[0]==0)
        printf("0 0");
        if(a1[i]>b1[j])
        {
            printf("%d %d",a[i],a1[i]);
            flag = 1;
            i++;
        }
        else if(a1[i]==b1[j])
        {
            if(a[i]+b[j]!=0)
            {
                printf("%d %d",a[i]+b[j],a1[i]);
                flag=1;
            }            
            i++;
            j++;
        }
        else
        {
            printf("%d %d",b[j],b1[j]);
            flag = 1;
            j++;
        }
    while(i<n1&&j<n2)
    {
        if(a1[i]>b1[j])
        {
            printf(" %d %d",a[i],a1[i]);
            i++;
        }
        else if(a1[i]==b1[j])
        {
            if(a[i]+b[j]!=0)
            printf(" %d %d",a[i]+b[j],a1[i]);
            i++;
            j++;
        }
        else
        {
            printf(" %d %d",b[j],b1[j]);
            j++;
        }
    }
    for(;i<n1;i++)
    {
        if(a[i])
        printf(" %d %d",a[i],a1[i]);
    }
    for(;j<n2;j++)
    {
        if(b[j])
        printf(" %d %d",b[j],b1[j]);
    }
    if(flag==-1)
    {
        printf("0 0");
    }
    return 0;
}
